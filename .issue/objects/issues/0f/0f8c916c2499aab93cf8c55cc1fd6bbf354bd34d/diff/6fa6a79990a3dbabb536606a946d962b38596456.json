[{"action": "open", "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703485}, {"action": "set-message", "params": {"text": "Rework imports\n\nThe `.import:` directive should work on modules instead of files. This\nwill require that all modules needed by an executable be already\nassembled before the executable can be assembled, but that is a\nreasonable requirement anyway.\n\nThis also means that the assembler will have to be able to look at\nVIUA_LIBRARY_PATH environment variable; a welcome change given that it\nwill force sanity into the modules-imports-libraries part of the VM..."}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703495}, {"action": "push-tags", "params": {"tags": ["enhancement", "important", "vm"]}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703498}, {"action": "push-milestones", "params": {"milestones": []}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703502}, {"action": "push-tags", "params": {"tags": ["viuavm"]}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703599}, {"action": "set-project-tag", "params": {"tag": "viuavm"}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703605}, {"action": "set-project-name", "params": {"name": "Viua Virtual Machine"}, "author": {"author.email": "marejm@ozro.pw", "author.name": "Marek Marecki"}, "timestamp": 1545391349.703609}]